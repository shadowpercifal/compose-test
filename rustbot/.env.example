# Discord
DISCORD_TOKEN=your-bot-token
DISCORD_CLIENT_ID=your-app-id
DISCORD_PUBLIC_KEY=your-public-key
DISCORD_GUILD_ID=your-guild-id

# Steam OpenID / OAuth (through reverse-proxy domain)
PUBLIC_BASE_URL=https://auth.fzbw.ru
SESSION_SECRET=change-me

# BattleMetrics
BATTLEMETRICS_API_KEY=your-battlemetrics-api-key
BATTLEMETRICS_SERVER_ID=your-rust-server-id

# Database
DATABASE_URL=postgresql://rustbot:password@rustbot-postgres:5432/rustbot

# Logging
LOG_LEVEL=info
LOG_TO_DISCORD=false
LOG_DISCORD_CHANNEL_ID=

# Cache
CACHE_TTL_SECONDS=60
CACHE_MAX_ITEMS=500