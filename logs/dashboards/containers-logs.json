{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true,
        "hide": true
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "type": "logs",
      "title": "Container Logs",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 20 },
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": true,
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true
      },
      "targets": [
        {
          "expr": "{job=\"docker-containers\"}",
          "queryType": "range"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate (last 5m)",
      "gridPos": { "x": 0, "y": 20, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(({job=\\\"docker-containers\\\"} |= \\\"error\\\")[5m]))"
        }
      ]
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["logs", "docker"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Container Logs",
  "version": 1
}